{
	"name": "estgroupe/laravel-taggable",
	"description": "Taggable Trait for using tag inside Laravel Eloquent models, with Baum's Nested Set pattern support.",
	"license": "MIT",
	"homepage": "https://github.com/summerblue/laravel-taggable",
	"keywords": ["tag", "tags", "tagging", "laravel", "taggable", "tagged", "eloquent", "laravel5"],
	"authors": [
		{
			"name": "Charlie Jade",
			"email": "cj@estgroupe.com"
		}
	],
	"require": {
		"php": ">=5.5.0",
		"illuminate/database": ">= 5.0",
		"illuminate/support": ">= 5.0",
		"baum/baum": "~1.1",
		"overtrue/pinyin" : "~3.0"
	},
	"require-dev": {
		"orchestra/testbench": "~3.0",
		"phpunit/phpunit": "~4.0",
		"mockery/mockery": "~0.9",
		"vlucas/phpdotenv": "~2.0"
	},
	"autoload": {
		"psr-4": {
			"EstGroupe\\Taggable\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	}
}
